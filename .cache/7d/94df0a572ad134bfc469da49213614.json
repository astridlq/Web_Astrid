rce":"stylesheets/style.scss","name":null,"original":{"line":50,"column":2},"generated":{"line":311,"column":2}},{"source":"stylesheets/style.scss","name":null,"original":{"line":55,"column":0},"generated":{"line":314,"column":0}},{"source":"stylesheets/style.scss","name":null,"original":{"line":57,"column":0},"generated":{"line":315,"column":0}},{"source":"stylesheets/style.scss","name":null,"original":{"line":58,"column":2},"generated":{"line":316,"column":2}}],"sources":{"stylesheets/style.scss":"// CONFIG\n\n// Import Google fonts\n@import url(\"https://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Raleway:400,100,300,700,500\");\n@import url('https://fonts.googleapis.com/css?family=Lora:400i,700i&display=swap');\n\n// Define fonts for body and headers\n$body-font: \"Open Sans\", \"Helvetica\", \"sans-serif\";\n$headers-font: \"Raleway\", \"Helvetica\", \"sans-serif\";\n$italic-font: \"Lora\", \"serif\";\n\n\n// Colors\n$red: #FE5F55;\n$blue: #0FA3B1;\n$yellow: #FFBA49;\n$orange: #F77402;\n$green: #5DEF62;\n$gray: #3B413C;\n$light-gray: #9DB5B2;\n$background-color: #F7F7F7;\n\n\n// Components import\n@import \"./components/_navbar.scss\";\n@import \"./components/_footer.scss\";\n@import \"./components/_banner.scss\";\n@import \"./components/_buttons.scss\";\n@import \"./components/_gallery.scss\";\n@import \"./components/_web-cards-projects.scss\";\n\n\n\n/* Main */\n\nbody {\n  margin: 0;\n  background-color: $background-color;\n}\n\na {\n  text-decoration: none;\n}\n\nh1, h2 {\n  font-family: $italic-font;\n}\n\n.container-main {\n  height: 100vh;\n}\n\n\n\n/* Sections Titles */\n\n.titles {\n  padding: 30px 0;\n}\n","stylesheets/components/_navbar.scss":"/* Navbar */\n\n.navbar-1 {\n  padding: 5px 20px;\n  position: fixed;\n  width: 100vw;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  z-index: 10;\n  p {\n    margin: 0;\n    font-size: 16px;\n  }\n  h3 {\n    margin: 0;\n    font-size: 18px;\n    text-decoration: none;\n  }\n}\n\n\n.nav-item {\n  font-weight: bold;\n  font-size: 14px;\n  // text-transform: uppercase;\n  text-decoration: none;\n  color: $blue;\n  padding: 20px 0px;\n  margin: 0px 20px;\n  display: inline-block;\n  position: relative;\n  opacity: 0.75;\n}\n\n.nav-item:hover {\n  a {\n    text-decoration: none;\n    color: $blue;\n    opacity: 1;\n  }\n}\n\n.nav-item::before {\n  transition: 300ms;\n  height: 2px;\n  content: \"\";\n  position: absolute;\n  background-color: $blue;\n}\n\n.nav-items-ltr::before {\n  width: 0%;\n  bottom: 15px;\n}\n\n.nav-items-ltr:hover::before {\n  width: 60%;\n}\n","stylesheets/components/_footer.scss":"/* Footer */\n\n.footer {\n  left: 0;\n  bottom: 0;\n  width: 100%;\n  background-color: $background-color;\n  height: 90px;\n  padding-left: 12%;\n  color: $light-gray;\n}\n.footer-links {\n  display: flex;\n  align-items: center;\n}\n.footer-links a {\n  color: $light-gray;\n  text-decoration: none;\n  font-size: 24px;\n  padding: 20px 0;\n  margin-right: 20px;\n}\n.footer-links a:hover {\n  opacity: 1;\n}\n\n","stylesheets/components/_banner.scss":"/* Banner */\n\n.banner-full {\n  height: 100vh;\n  width: 100vw;\n  position: absolute;\n  background-size: cover;\n  background-position: top;\n  background-image: linear-gradient(rgba(0,0,0,0.2), rgba(0,0,0,0.2)), url(\"https://images.unsplash.com/photo-1521811628991-7a3ea581f7d1?ixlib=rb-1.2.1&auto=format&fit=crop&w=1050&q=80\");\n  background-attachment: fixed;\n  z-index: -2;\n}\n\n.header-banner {\n  display: flex;\n  justify-content: space-around;\n  color: white;\n  text-shadow: 0 0 7px rgba(0,0,0,0.2);\n  font-weight: 700em;\n  line-height: 1.5;\n  .header-quote {\n    font-family: $italic-font;\n    font-size: 40px;\n    padding-top: 20%;\n  }\n  h2 {\n    font-size: 16px;\n  }\n}\n","stylesheets/components/_buttons.scss":".button {\n  color: $blue;\n  width: 100%;\n  margin: 10px;\n  padding: 20px;\n  border: 2px solid $blue;\n  text-align: center;\n  text-transform: uppercase;\n  position: relative;\n  overflow:hidden;\n  transition: .3s;\n  &:after {\n    position: absolute;\n    transition: .3s;\n    content: '';\n    width: 0;\n    left: 50%;\n    bottom: 0;\n    height: 3px;\n    background: $blue;\n  }\n  &.button:after {\n    height: 120%;\n    left: -10%;\n    transform: skewX(15deg);\n    z-index: -1;\n  }\n  &:hover {\n    cursor: pointer;\n    &:after {\n      width: 100%;\n      left: 0;\n    }\n    &.button {\n      color: white;\n    }\n    &.button:after {\n      left: -10%;\n      width: 120%;\n    }\n  }\n}\n\n.arrow:before{\n  content:\"→\";\n  position:absolute;\n  color:white;\n  left: 83%;\n  opacity: 0;\n  -webkit-transition: all 250ms cubic-bezier(0.680, -0.550, 0.265, 1.550);\n}\n\n.arrow:hover:before{\n  left:84%;\n  opacity:1;\n}\n","stylesheets/components/_gallery.scss":".gallery {\n  display: grid;\n  grid-template-columns: repeat(8, 1fr);\n  grid-template-rows: repeat(8, 5vw);\n  grid-gap: 15px;\n}\n\n@media(max-width: 768px) {\n  .cards {\n    grid-template-columns: 1fr;\n  }\n}\n\n\n.gallery__img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n.gallery__item--1 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 1;\n  grid-column-end: 3;\n  grid-row-start: 1;\n  grid-row-end: 3;\n}\n\n.gallery__item--2 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 3;\n  grid-column-end: 5;\n  grid-row-start: 1;\n  grid-row-end: 3;\n}\n\n.gallery__item--3 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 5;\n  grid-column-end: 9;\n  grid-row-start: 1;\n  grid-row-end: 6;\n}\n\n.gallery__item--4 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 1;\n  grid-column-end: 5;\n  grid-row-start: 3;\n  grid-row-end: 6;\n}\n\n.gallery__item--5 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 1;\n  grid-column-end: 5;\n  grid-row-start: 6;\n  grid-row-end: 9;\n}\n\n.gallery__item--6 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 5;\n  grid-column-end: 9;\n  grid-row-start: 6;\n  grid-row-end: 9;\n}\n\n","stylesheets/components/_web-cards-projects.scss":".container-web-projects {\n  margin-bottom: 30px;\n}\n\n.card-columns {\n  a{\n    color: $gray;\n  }\n  a:hover {\n    color: $gray;\n    text-decoration: none;\n  }\n}\n\n.web-card {\n  margin-bottom: 25px;\n  background-color: white;\n  border-radius: 2px;\n  box-shadow: 0 0 10px rgba(0,0,0,0.2);\n  font-size: 18px;\n  opacity: 0.9;\n  display: inline-block;\n  &:hover {\n    opacity: 1;\n  }\n  h3 {\n    font-size: 21px;\n    font-weight: bold;\n    margin: 8px 0;\n  }\n  p {\n    font-size: 12px;\n    line-height: 1.4;\n    font-weight: 500em;\n    opacity: .7;\n    margin-bottom: 0;\n  }\n  img {\n    // height: auto;\n    width: 100%;\n    object-fit: cover;\n  }\n  .info-web-card {\n    // height: auto;\n    padding: 20px;\n    vertical-align: middle;\n  }\n}\n\n.tags {\n  color: $gray;\n  font-size: 10px;\n  font-family: $italic-font;\n}\n\n"},"lineCount":null}},"error":null,"hash":"219d2a5ca2974157d614c4244645486b","cacheData":{"env":{}}}