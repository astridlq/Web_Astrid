full {\n  height: 100vh;\n  width: 100vw;\n  position: absolute;\n  background-size: cover;\n  background-position: top;\n  background-image: linear-gradient(rgba(0,0,0,0.2), rgba(0,0,0,0.2)), url(\"https://images.unsplash.com/photo-1521811628991-7a3ea581f7d1?ixlib=rb-1.2.1&auto=format&fit=crop&w=1050&q=80\");\n  background-attachment: fixed;\n  z-index: -2;\n}\n\n.header-banner {\n  display: flex;\n  justify-content: space-around;\n  color: white;\n  text-shadow: 0 0 7px rgba(0,0,0,0.2);\n  font-weight: 700em;\n  line-height: 1.5;\n  .header-quote {\n    font-family: $italic-font;\n    font-size: 40px;\n    padding-top: 20%;\n  }\n  h2 {\n    font-size: 16px;\n  }\n}\n\n/* Intro */\n\n.container-intro {\n  width: 74%;\n  margin: 20px 0;\n}\n\n.intro-text {\n  font-style: italic;\n  color: white;\n  background-color: #FE5F55;\n  padding: 50px;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: -35px;\n    color: $gray;\n    font-size: 50px;\n  }\n}\n\n\n@media (max-width: 1200px) {\n  .intro-text {\n    margin: 20px;\n  }\n}\n\n\n@media (max-width: 575px) {\n  .intro-text {\n    font-size: 12px;\n    padding: 20px;\n  }\n}\n\n\n\n/* Button */\n\n.button {\n  color: $blue;\n  width: 100%;\n  margin: 10px;\n  padding: 20px;\n  border: 2px solid $blue;\n  text-align: center;\n  text-transform: uppercase;\n  position: relative;\n  overflow:hidden;\n  transition: .3s;\n  &:after {\n    position: absolute;\n    transition: .3s;\n    content: '';\n    width: 0;\n    left: 50%;\n    bottom: 0;\n    height: 3px;\n    background: $blue;\n  }\n  &.button:after {\n    height: 120%;\n    left: -10%;\n    transform: skewX(15deg);\n    z-index: -1;\n  }\n  &:hover {\n    cursor: pointer;\n    &:after {\n      width: 100%;\n      left: 0;\n    }\n    &.button {\n      color: white;\n    }\n    &.button:after {\n      left: -10%;\n      width: 120%;\n    }\n  }\n}\n\n.arrow:before{\n  content:\"â†’\";\n  position:absolute;\n  color:white;\n  left: 83%;\n  opacity: 0;\n  -webkit-transition: all 250ms cubic-bezier(0.680, -0.550, 0.265, 1.550);\n}\n\n.arrow:hover:before{\n  left:84%;\n  opacity:1;\n}\n\n/* Pictures gallery */\n\n.gallery {\n  display: grid;\n  grid-template-columns: repeat(8, 1fr);\n  grid-template-rows: repeat(8, 5vw);\n  grid-gap: 15px;\n}\n\n@media(max-width: 768px) {\n  .cards {\n    grid-template-columns: 1fr;\n  }\n}\n\n\n.gallery__img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n.gallery__item--1 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 1;\n  grid-column-end: 3;\n  grid-row-start: 1;\n  grid-row-end: 3;\n}\n\n.gallery__item--2 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 3;\n  grid-column-end: 5;\n  grid-row-start: 1;\n  grid-row-end: 3;\n}\n\n.gallery__item--3 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 5;\n  grid-column-end: 9;\n  grid-row-start: 1;\n  grid-row-end: 6;\n}\n\n.gallery__item--4 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 1;\n  grid-column-end: 5;\n  grid-row-start: 3;\n  grid-row-end: 6;\n}\n\n.gallery__item--5 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 1;\n  grid-column-end: 5;\n  grid-row-start: 6;\n  grid-row-end: 9;\n}\n\n.gallery__item--6 {\n  margin: 0;\n  background-color: $blue;\n  grid-column-start: 5;\n  grid-column-end: 9;\n  grid-row-start: 6;\n  grid-row-end: 9;\n}\n\n\n\n/* Footer */\n\n.footer {\n  left: 0;\n  bottom: 0;\n  width: 100%;\n  background-color: $background-color;\n  height: 90px;\n  padding-left: 12%;\n  color: $light-gray;\n}\n.footer-links {\n  display: flex;\n  align-items: center;\n}\n.footer-links a {\n  color: $light-gray;\n  text-decoration: none;\n  font-size: 24px;\n  padding: 20px 0;\n  margin-right: 20px;\n}\n.footer-links a:hover {\n  opacity: 1;\n}\n\n"},"lineCount":232}},"error":null,"hash":"721a2df2fdd265362f36bf6578236f03","cacheData":{"env":{}}}